            priority="4"
            scenario="DICE::DICE_UML_Extensions::DTSM::Hadoop::HadoopScenario">
      </AnalyzableResourceBuilder>
      <AnalyzableResourceBuilder
            analyzableProperty="RELIABILITY"
            builder="es.unizar.disco.pnml.m2m.builder.HadoopScenario2PnmlReliabilityResourceBuilder"
            metaclass="org.eclipse.uml2.uml.Activity"
            priority="4"
            scenario="DICE::DICE_UML_Extensions::DTSM::Hadoop::HadoopScenario">
      </AnalyzableResourceBuilder>
      <AnalyzableResourceBuilder
            analyzableProperty="PERFORMANCE"
            builder="es.unizar.disco.pnml.m2m.builder.HadoopActivityDiagram2PnmlResourceBuilder"
            metaclass="org.eclipse.uml2.uml.Activity"
            priority="4"
            scenario="MARTE::MARTE_AnalysisModel::GQAM::GaScenario">
      </AnalyzableResourceBuilder>
      <AnalyzableResourceBuilder
            analyzableProperty="RELIABILITY"
            builder="es.unizar.disco.pnml.m2m.builder.StormScenario2PnmlReliabilityResourceBuilder"
            metaclass="org.eclipse.uml2.uml.Activity"
            priority="4"
            scenario="DICE::DICE_UML_Extensions::DTSM::Storm::StormScenarioTopology">
      </AnalyzableResourceBuilder>
      <AnalyzableResourceBuilder
            analyzableProperty="PERFORMANCE"
            builder="es.unizar.disco.pnml.m2m.builder.SparkActivityDiagram2PnmlResourceBuilder"
            metaclass="org.eclipse.uml2.uml.Activity"
            priority="4"
            scenario="DICE::DICE_UML_Extensions::DTSM::Spark::SparkScenario">
      </AnalyzableResourceBuilder>
      <AnalyzableResourceBuilder
            analyzableProperty="PERFORMANCE"
            builder="es.unizar.disco.pnml.m2m.builder.SparkActivityDiagram2PnmlResourceBuilder"
            metaclass="org.eclipse.uml2.uml.Activity"
            priority="4"
            scenario="MARTE::MARTE_AnalysisModel::GQAM::GaScenario">
      </AnalyzableResourceBuilder>
      <AnalyzableResourceBuilder
            analyzableProperty="RELIABILITY"
            builder="es.unizar.disco.pnml.m2m.builder.SparkScenario2PnmlReliabilityResourceBuilder"
            metaclass="org.eclipse.uml2.uml.Activity"
            priority="4"
            scenario="DICE::DICE_UML_Extensions::DTSM::Spark::SparkScenario">
      </AnalyzableResourceBuilder>
   </extension>
</plugin>
